import csv

"""Reads delimeted files"""
class DelimitedReader:

    
    def __init__(self):
        pass
    
    
    
    def read(self, filename,func):
        with open(filename,'rb')as tsvin:
            tsvin = csv.reader(tsvin,delimiter='\t')

            for row in tsvin:
                print row
                func(row)
                
    def test(self,func):
        func()
        
    def say(self):
        print('asdasdas')

        

            